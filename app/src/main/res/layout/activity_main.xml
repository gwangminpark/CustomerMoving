<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!--<android.support.v4.widget.DrawerLayout-->

    <!--xmlns:tools="http://schemas.android.com/tools"-->
    <!--android:id="@+id/drawer_layout"-->
    <!--android:layout_width="match_parent"-->
    <!--android:layout_height="match_parent"-->
    <!--android:fitsSystemWindows="true"-->
    <!--tools:openDrawer="start">-->
    <android.support.v4.widget.DrawerLayout
        android:id="@+id/drawer_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true"
        android:focusableInTouchMode="true">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:id="@+id/linearLayout">


                <include
                    android:id="@+id/common_toolbar"
                    layout="@layout/common_toolbar" />

                <LinearLayout
                    android:id="@+id/fragment_container"
                    android:layout_width="match_parent"
                    android:layout_height="581dp"
                    android:background="@color/sky"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="360dp"
                        android:layout_height="460dp"
                        android:orientation="horizontal">

                        <RelativeLayout
                            android:layout_width="80dp"
                            android:layout_height="428dp"
                            android:layout_marginTop="20dp"
                            android:background="@color/sky">


                            <ImageView
                                android:id="@+id/imageView13"
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:layout_alignParentTop="true"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="32dp"
                                android:background="@drawable/check_1" />

                            <ImageView
                                android:id="@+id/imageView14"
                                android:layout_width="3dp"
                                android:layout_height="55dp"
                                android:layout_below="@+id/imageView13"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="10dp"
                                android:background="@drawable/line_01" />

                            <ImageView
                                android:id="@+id/day_left_circle"
                                android:layout_width="15dp"
                                android:layout_height="15dp"
                                android:layout_alignStart="@+id/imageView13"
                                android:layout_below="@+id/imageView14"
                                android:layout_marginTop="5dp"
                                android:background="@drawable/check_0" />

                            <ImageView
                                android:id="@+id/imageView16"
                                android:layout_width="3dp"
                                android:layout_height="60dp"
                                android:layout_alignStart="@+id/imageView14"
                                android:layout_below="@+id/day_left_circle"
                                android:layout_marginTop="9dp"
                                android:background="@drawable/line_01"
                                android:scaleType="fitXY" />

                            <ImageView
                                android:id="@+id/start_left_circle"
                                android:layout_width="15dp"

                                android:layout_height="15dp"
                                android:layout_alignStart="@+id/day_left_circle"
                                android:layout_below="@+id/imageView16"
                                android:layout_marginTop="9dp"
                                android:background="@drawable/check_0" />

                            <ImageView
                                android:id="@+id/imageView18"
                                android:layout_width="3dp"

                                android:layout_height="60dp"
                                android:layout_alignStart="@+id/imageView16"
                                android:layout_below="@+id/start_left_circle"
                                android:layout_marginTop="10dp"
                                android:background="@drawable/line_01" />

                            <ImageView
                                android:id="@+id/finish_left_circle"
                                android:layout_width="15dp"

                                android:layout_height="15dp"
                                android:layout_alignStart="@+id/start_left_circle"
                                android:layout_below="@+id/imageView18"
                                android:layout_marginTop="7dp"
                                android:background="@drawable/check_0" />

                            <ImageView
                                android:layout_width="15dp"
                                android:layout_height="95dp"
                                android:layout_alignStart="@+id/finish_left_circle"
                                android:layout_below="@+id/finish_left_circle"
                                android:layout_marginTop="8dp"
                                android:scaleType="fitXY"
                                android:background="@drawable/line_02" />

                            <ImageView

                                android:id="@+id/car"
                                android:layout_width="50dp"
                                android:layout_height="40dp"
                                android:layout_alignParentTop="true"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="23dp"
                                android:background="@drawable/truck" />


                        </RelativeLayout>

                        <LinearLayout
                            android:layout_width="280dp"
                            android:layout_height="428dp"
                            android:layout_marginTop="30dp"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:id="@+id/movingtype_btn"
                                android:layout_width="match_parent"
                                android:layout_height="84dp"
                                android:background="@color/sky">

                                <ImageView
                                    android:id="@+id/imageView2"
                                    android:layout_width="95dp"
                                    android:layout_height="63dp"
                                    android:layout_alignParentStart="true"
                                    android:layout_alignParentTop="true"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/main_m01" />

                                <ImageView
                                    android:id="@+id/imageView8"
                                    android:layout_width="161dp"
                                    android:layout_height="63dp"
                                    android:layout_alignParentTop="true"
                                    android:layout_toEndOf="@+id/imageView2"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/main_bg" />

                                <TextView
                                    android:id="@+id/moving_type"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"

                                    android:layout_alignParentTop="true"
                                    android:layout_marginStart="28dp"
                                    android:layout_marginTop="19dp"
                                    android:layout_toEndOf="@+id/imageView2"
                                    android:text="이사종류"
                                    android:textColor="#1E1E1E"
                                    android:textSize="18sp"
                                    android:textStyle="bold" />

                                <ImageView
                                    android:id="@+id/imageView"
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"

                                    android:layout_alignParentBottom="true"
                                    android:layout_marginBottom="71dp"
                                    android:layout_toEndOf="@+id/imageView8"
                                    android:background="@drawable/call_t"
                                    android:visibility="gone" />

                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/movingday_btn"
                                android:layout_width="match_parent"
                                android:layout_height="84dp"
                                android:layout_marginTop="4dp">

                                <ImageView
                                    android:id="@+id/imageView3"
                                    android:layout_width="95dp"
                                    android:layout_height="63dp"
                                    android:scaleType="fitXY"
                                    android:layout_alignParentStart="true"
                                    android:layout_alignParentTop="true"
                                    android:src="@drawable/main_m02" />

                                <ImageView
                                    android:id="@+id/imageView9"
                                    android:layout_width="161dp"
                                    android:layout_height="63dp"
                                    android:scaleType="fitXY"
                                    android:layout_alignParentTop="true"
                                    android:layout_toEndOf="@+id/imageView3"
                                    android:src="@drawable/main_bg" />

                                <TextView
                                    android:id="@+id/day"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"


                                    android:layout_alignParentTop="true"
                                    android:layout_marginStart="29dp"
                                    android:layout_marginTop="19dp"
                                    android:layout_toEndOf="@+id/imageView3"
                                    android:text="이사날짜"
                                    android:textColor="#1E1E1E"
                                    android:textSize="18sp"
                                    android:textStyle="bold" />

                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/movingstart_btn"
                                android:layout_width="match_parent"
                                android:layout_height="84dp"
                                android:layout_marginTop="4dp">

                                <ImageView
                                    android:id="@+id/imageView4"
                                    android:layout_width="95dp"
                                    android:layout_height="63dp"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/main_m03" />

                                <ImageView
                                    android:id="@+id/imageView10"
                                    android:layout_width="161dp"
                                    android:layout_height="63dp"
                                    android:layout_alignParentTop="true"
                                    android:layout_toEndOf="@+id/imageView4"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/main_bg" />

                                <TextView
                                    android:id="@+id/startday"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"


                                    android:layout_alignBottom="@+id/imageView10"
                                    android:layout_marginBottom="19dp"
                                    android:layout_marginStart="29dp"
                                    android:layout_toEndOf="@+id/imageView4"
                                    android:text="출발주소"
                                    android:textColor="#1E1E1E"
                                    android:textSize="18sp"
                                    android:textStyle="bold" />
                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/movingfinish_btn"
                                android:layout_width="match_parent"
                                android:layout_height="84dp"
                                android:layout_marginTop="4dp">

                                <ImageView
                                    android:id="@+id/imageView5"
                                    android:layout_width="93dp"
                                    android:layout_height="63dp"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/main_m04" />

                                <ImageView
                                    android:id="@+id/imageView11"
                                    android:layout_width="161dp"
                                    android:layout_height="63dp"
                                    android:scaleType="fitXY"
                                    android:layout_alignParentTop="true"
                                    android:layout_toEndOf="@+id/imageView5"
                                    android:src="@drawable/main_bg" />


                                <TextView
                                    android:id="@+id/finishday"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"


                                    android:layout_alignParentTop="true"
                                    android:layout_marginStart="33dp"
                                    android:layout_marginTop="19dp"
                                    android:layout_toEndOf="@+id/imageView5"
                                    android:text="도착주소"
                                    android:textColor="#1E1E1E"
                                    android:textSize="18sp"
                                    android:textStyle="bold" />

                            </RelativeLayout>

                            <RelativeLayout
                                android:id="@+id/movingphone_btn"
                                android:layout_width="match_parent"
                                android:layout_height="84dp"
                                android:layout_marginTop="4dp">

                                <ImageView
                                    android:id="@+id/imageView6"
                                    android:layout_width="93dp"
                                    android:layout_height="63dp"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/main_m05" />

                                <ImageView
                                    android:id="@+id/imageView12"
                                    android:layout_width="161dp"
                                    android:layout_height="63dp"
                                    android:scaleType="fitXY"
                                    android:layout_alignParentTop="true"
                                    android:layout_toEndOf="@+id/imageView6"
                                    android:src="@drawable/main_bg" />


                                <TextView
                                    android:id="@+id/moving_phone"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"


                                    android:layout_alignParentTop="true"
                                    android:layout_marginStart="33dp"
                                    android:layout_marginTop="19dp"
                                    android:layout_toEndOf="@+id/imageView6"
                                    android:text="연락처"
                                    android:textColor="#1E1E1E"
                                    android:textSize="18sp"
                                    android:textStyle="bold" />

                            </RelativeLayout>


                        </LinearLayout>


                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:orientation="horizontal">


                    </LinearLayout>
                </LinearLayout>

                <!--<include-->
                <!--layout="@layout/app_bar_main"-->
                <!--android:layout_width="match_parent"-->
                <!--android:layout_height="match_parent" />-->

                <!--<android.support.design.widget.NavigationView-->
                <!--android:id="@+id/nav_view"-->
                <!--android:layout_width="wrap_content"-->
                <!--android:layout_height="match_parent"-->
                <!--android:layout_gravity="start"-->
                <!--android:fitsSystemWindows="true"-->
                <!--app:headerLayout="@layout/nav_header_main"-->
                <!--app:menu="@menu/activity_main_drawer" />-->
            </LinearLayout>

            <RelativeLayout

                android:id="@+id/callbt"
                android:layout_width="75dp"
                android:layout_height="70dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:layout_marginEnd="18dp"
                android:layout_marginTop="12dp">

                <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:background="@drawable/call_t" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true"
                    android:text="1666-2428"
                    android:textColor="@color/yellow"
                    android:textStyle="bold" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/search_bt"
                android:layout_width="320dp"
                android:layout_height="70dp"

                android:layout_alignBottom="@+id/linearLayout"
                android:layout_alignParentStart="true"
                android:layout_marginBottom="25dp"
                android:layout_marginStart="22dp"
                android:background="#65005FC9">



                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:text="견적신청하기"
                    android:textColor="#FFFFFF"
                    android:textSize="19sp"
                    android:textStyle="bold" />


            </RelativeLayout>
        </RelativeLayout>
        <android.support.design.widget.NavigationView
            android:id="@+id/nav_view"
            android:layout_width="145dp"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:fitsSystemWindows="true"
            android:overScrollMode="never"
            android:scrollbars="none">

            <include
                android:id="@+id/common_navigation"
                layout="@layout/common_navigation" />
        </android.support.design.widget.NavigationView>


    </android.support.v4.widget.DrawerLayout>
</layout>